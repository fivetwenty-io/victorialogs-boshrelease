check process victorialogs
  with pidfile /var/vcap/sys/run/bpm/victorialogs/victorialogs.pid
  start program "/var/vcap/jobs/bpm/bin/bpm start victorialogs"
  stop program "/var/vcap/jobs/bpm/bin/bpm stop victorialogs"
  group vcap
  if failed host 127.0.0.1 port <%= p('victorialogs.port') %> protocol http
     request /health
     with timeout 10 seconds
     for 3 cycles
  then restart
